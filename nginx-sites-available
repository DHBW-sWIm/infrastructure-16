# Wiki

server {
	server_name wiki.moodle-dhbw.de;
	location / {
		proxy_pass http://192.168.200.12:8080;
		proxy_set_header Host $host;
		proxy_set_header X-HTTPS-Protocol $ssl_protocol;
	}


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/bpxtest.moodle-dhbw.de/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/bpxtest.moodle-dhbw.de/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

